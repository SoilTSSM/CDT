{
    // Input and Output files/directory
    "IO.files": [{
        // Station data filename
        "STN.file1": "",
        // Station data filename
        "STN.file2": "",
        // Elevation data filename
        "DEM.file": "",
        // Full path to the directory to save results
        "dir2save": ""
    }],
    // Data to control Tmax?
    "is.TX": true,
    // Perform consistency check Tmax < Tmin?
    "consist.check": false,
    "dem": [{
        // use elevation data to select neighbor stations
        "use.elv": false,
        // Elevation data from STN data: "1", interpolate from dem: "0" 
        "interp.dem": "0"
    }],
    "stn.type": [{
        // series from one station
        "single.series": false,
        // one variable: "1", 3 variables: "0" 
        "file.format": "1",
        // grouped date: "1", separated: "0" 
        "date.format": "1"
    }],
    "select.pars": [{
        // Minimum number of neighbor stations to use
        "min.stn": 2,
        // Maximum number of neighbor stations to use
        "max.stn": 7,
        // Time window: number of day/dek/month to be used for regression
        "win.len": 30,
        // Maximum distance of neighbor stations to use (km)
        "max.dist": 50,
        // Maximum altitude difference of neighbor stations to use (m)
        "elv.diff": 200,
        // Confidence level (%)
        "conf.lev": 99.73
    }],
    "limits": [{
        "Lower.Bounds": -10,
        "Upper.Bounds": 60
    }],
    "AllOrOne": "one"
}
